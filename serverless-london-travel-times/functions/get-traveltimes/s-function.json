{
  "name": "get-traveltimes",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: serverless-london-travel-times",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "get-traveltimes",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters":  {"integration.request.querystring.destination": "method.request.querystring.destination"},
      "requestTemplates": {
        "application/json": "{\n\"destination\": \"$input.params('destination')\"}"
      },
      "responses": {
        "400": {
          "statusCode": "400",
          "responseParameters": {
             "method.response.header.Access-Control-Allow-Headers": "'Content-Type,X-Amz-Date,Authorization,X-Api-Key,Cache-Control'",
             "method.response.header.Access-Control-Allow-Methods": "'*'",
             "method.response.header.Access-Control-Allow-Origin": "'*'"
           }
        },
        "default": {
          "statusCode": "200",
          "responseParameters":{
            "method.response.header.Access-Control-Allow-Headers": "'Content-Type,X-Amz-Date,Authorization,X-Api-Key,Cache-Control'",
            "method.response.header.Access-Control-Allow-Methods": "'*'",
            "method.response.header.Access-Control-Allow-Origin": "'*'"
          },
          "responseModels": {
            "application/json;charset=UTF-8": "Empty"
          },
          "responseTemplates": {
            "application/json;charset=UTF-8": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
